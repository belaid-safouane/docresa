                    <div class="col l12 s12">
                        <div class="card">
                            <div class="card-content">
                                <div class="d-flex no-block align-items-center">
                                    <h5 class="card-title">All Contacts</h5>
                                    <div class="ml-auto">
                                        <a class="waves-effect waves-light btn blue-grey darken-4 modal-trigger" href="{{path('ads_create')}}">Create New Contact</a>
                                    </div>
                                </div>
                                <div class="table-responsive">
                                    <table id="file_export" class="table table-bordered nowrap display">
                                        <thead>
                                            <tr>
                                                <th> </th>
                                                <th>Name</th>
                                                <th>Email</th>
                                                <th>Phone</th>
                                                <th>Role</th>
                                                <th>Age</th>
                                                <th>Joining date</th>
                                                <th>Salery</th>
                                                <th>Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            {% for ad in ads %}
                                            <tr>
                                                <td>
                                                    <p>
                                                        <label class="m-l-15">
                                                            <input type="checkbox" />
                                                            <span></span>
                                                        </label>
                                                    </p>
                                                </td>
                                                <td>
                                                    <a href="app-contact-detail.html"><img src="/assets/images/users/1.jpg" alt="user" class="circle" width="30px" />{{ad.name}} </a>
                                                </td>
                                                <td>{{ad.author.firstname}}</td>
                                                <td>+123 456 789</td>
                                                <td><span class="label label-danger">{{ad.category}}</span> </td>
                                                <td>23</td>
                                                <td>12-10-2014</td>
                                                <td>$1200</td>
                                                <td>
                                                    <button type="button" class="btn btn-sm btn-icon btn-pure btn-outline delete-row-btn" data-toggle="tooltip" data-original-title="Delete">
                                                        <a href="{{ path('delete_account_ad_register', {'slug':ad.slug }) }}">
                                                        <i class="ti-close" aria-hidden="true"></i></a>
                                                    
                                                    </button>
                                                </td>
                                            </tr>
                                            {% endfor %}
